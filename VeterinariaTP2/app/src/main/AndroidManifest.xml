<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neo.veterinaria">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Veterinaria">
        <activity android:name=".view.MainActivity"></activity>
        <activity android:name=".view.SalaDeEsperaActivity" />
        <activity android:name=".view.AtenderActivity" />
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".model.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>


